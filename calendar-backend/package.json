{
  "name": "calendar-backend",
  "version": "1.0.0",
  "description": "Backend for Google Calendar integration with Express and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "express",
    "google-calendar",
    "postgresql",
    "drizzle"
  ],
  "author": "Karlo Majer",
  "license": "ISC",
  "dependencies": {
    "drizzle-orm": "^0.33.0",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "googleapis": "^128.0.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "drizzle-kit": "^0.24.0",
    "tsx": "^4.7.0",
    "@types/pg": "^8.10.9",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3"
  }
}
