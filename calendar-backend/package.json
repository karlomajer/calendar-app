{
  "name": "calendar-backend",
  "version": "1.0.0",
  "description": "Backend for Google Calendar integration with Express and PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "keywords": [
    "express",
    "google-calendar",
    "postgresql",
    "drizzle"
  ],
  "author": "Karlo Majer",
  "license": "ISC",
  "dependencies": {
    "drizzle-orm": "^0.44.7",
    "pg": "^8.16.3",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "googleapis": "^128.0.0",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.5",
    "tsx": "^4.20.6",
    "@types/pg": "^8.15.5",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "typescript": "^5.9.3"
  }
}
